+package org.androidtown.backkeyapplication;
+
+/**
+ * Created by RL on 2016-04-05.
+ * 연속으로 2초 이내에 키 입력이 있으면 종료.
+ */
+import android.app.Activity;
+import android.widget.Toast;
+
+public class BackPressCloseHandler {
+    private long backKeyPressedTime = 0;    // 시간 지정.
+    private Toast toast;
+    private Activity activity;
+
+    public BackPressCloseHandler(Activity context){
+        this.activity = context;
+    }
+
+    public void onBackPressed(){
+        if (System.currentTimeMillis() > backKeyPressedTime + 2000) { //시간이 2초보다 작다.
+            backKeyPressedTime = System.currentTimeMillis();
+            showGuide();    //알림창을 표시한다.
+            return;
+        }
+        if (System.currentTimeMillis() <= backKeyPressedTime + 2000) { //시간이 2초보다 작다
+            activity.finish();  // 종료한다.
+            toast.cancel();
+            //System.exit(0);
+        }
+    }
+
+    public void showGuide() {
+        toast = Toast.makeText(activity,
+                "\'뒤로\'버튼을 한번 더 누르시면 종료됩니다.", Toast.LENGTH_SHORT);
+        toast.show();
+    }
+}
