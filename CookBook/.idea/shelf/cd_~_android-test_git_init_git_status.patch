+package org.androidtown.exam1application;
+
+import android.content.Intent;
+import android.net.Uri;
+import android.support.v7.app.AppCompatActivity;
+import android.os.Bundle;
+import android.text.Editable;
+import android.text.TextWatcher;
+import android.view.View;
+import android.widget.Button;
+import android.widget.EditText;
+import android.widget.ImageView;
+import android.widget.RadioGroup;
+import android.widget.Toast;
+import com.google.android.gms.common.api.GoogleApiClient;
+
+public class MainActivity extends AppCompatActivity {
+
+    String inputString = "";
+    /**
+     * ATTENTION: This was auto-generated to implement the App Indexing API.
+     * See https://g.co/AppIndexing/AndroidStudio for more information.
+     */
+//    private GoogleApiClient client;
+
+    @Override
+    protected void onCreate(Bundle savedInstanceState) {
+        super.onCreate(savedInstanceState);
+        setContentView(R.layout.activity_main);
+
+        final EditText mInputText = (EditText) findViewById(R.id.inputText);
+        Button mDisplayTextButton = (Button) findViewById(R.id.displayTextButton);
+        Button mOpenWebButton = (Button) findViewById(R.id.openWebButton);
+        final RadioGroup mImageRadioGroup = (RadioGroup)findViewById(R.id.imageradioGroup);
+        final ImageView mImageView = (ImageView)findViewById(R.id.imageView);
+
+        mInputText.addTextChangedListener(new TextWatcher() {
+            @Override
+            public void beforeTextChanged(CharSequence s, int start, int count, int after) {
+            }
+
+            @Override
+            public void onTextChanged(CharSequence s, int start, int before, int count) {
+            }
+
+            @Override
+            public void afterTextChanged(Editable s) {
+                inputString = mInputText.getText().toString();
+                Toast.makeText(getApplicationContext(),inputString, 0).show();
+            }
+        });
+
+        mDisplayTextButton.setOnClickListener(new View.OnClickListener() {
+            @Override
+            public void onClick(View v) {
+                inputString = mInputText.getText().toString();
+                if (inputString.trim().equals("")) {
+                    Toast.makeText(getApplicationContext(), "입력값이 비었습니다.", 0).show();
+                } else {
+                    Toast.makeText(getApplicationContext(), inputString, Toast.LENGTH_SHORT).show();
+                }
+            }
+        });
+
+        mOpenWebButton.setOnClickListener(new View.OnClickListener() {
+            @Override
+            public void onClick(View v) {
+                inputString = mInputText.getText().toString();
+                if (inputString.trim().equals("")) {
+                    Toast.makeText(getApplicationContext(), "입력값이 비었습니다.", 0).show();
+                } else if(android.util.Patterns.WEB_URL.matcher(inputString).matches()) {
+                    Intent mIntent = new Intent(Intent.ACTION_VIEW, Uri.parse("http://" + inputString));
+                    startActivity(mIntent);
+                } else {
+                    Toast.makeText(getApplicationContext(), "올바른 웹주소를 입력하세요.", 0).show();
+                }
+            }
+        });
+
+        mImageRadioGroup.setOnCheckedChangeListener(new RadioGroup.OnCheckedChangeListener() {
+            @Override
+            public void onCheckedChanged(RadioGroup group, int checkedId) {
+                switch (checkedId) {
+                    case R.id.image1radioButton:
+                        Toast.makeText(getApplicationContext(), "Radio Button 1.", 0).show();
+                        mImageView.setImageResource(R.drawable.chrysanthemum);
+                        break;
+                    case R.id.image2radioButton:
+                        Toast.makeText(getApplicationContext(), "Radio Button 2.", 0).show();
+                        mImageView.setImageResource(R.drawable.tulips);
+                        break;
+                }
+            }
+        });
+    }
+}
